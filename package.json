{
  "name": "react-redux-boilerplate",
  "author": "Raice Hannay <voodoocreation@gmail.com>",
  "description": "A React Redux boilerplate with server rendering.",
  "license": "ISC",
  "version": "1.0.0",
  "keywords": [
    "react",
    "redux",
    "redux-saga",
    "next",
    "server rendering",
    "SSR",
    "boilerplate"
  ],
  "scripts": {
    "dev": "cross-env PORT=5000 node --icu-data-dir=node_modules/full-icu next.server.js",
    "build": "npm-run-all clean build:prod",
    "build:prod": "next build",
    "clean": "rimraf dist",
    "fmt": "prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
    "fmt:p": "prettier-package-json --write",
    "start": "cross-env pm2 start start.config.json --env=production",
    "stop": "cross-env pm2 kill",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --config .eslintrc \"./{types,src,pages}/**/*.js?(x)\" --ext .js,.jsx",
    "lint:scss": "stylelint -c .eslintrc \"src/scss/**/**/**/**/*.+(css|scss)\"",
    "lint:ts": "tslint \"./{types,src,pages}/**/*.ts?(x)\"",
    "precommit": "lint-staged",
    "test": "cross-env NODE_ENV=test jest --no-cache --env=jsdom --config ./jest.config.js"
  },
  "dependencies": {
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-to-json": "^1.5.2",
    "@types/humps": "^1.1.2",
    "@types/jest": "^23.3.1",
    "@types/next": "^6.1.4",
    "@types/next-redux-wrapper": "^2.0.0",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-intl": "^2.3.10",
    "@types/react-redux": "^6.0.6",
    "@zeit/next-typescript": "^1.1.0",
    "accepts": "^1.3.4",
    "autoprefixer": "^9.1.3",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^7.0.0",
    "enzyme": "^3.5.1",
    "enzyme-adapter-react-16": "^1.3.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "full-icu": "^1.2.1",
    "glob": "^7.1.3",
    "group-css-media-queries-loader": "^2.0.2",
    "humps": "^2.0.1",
    "image-size-loader": "^0.7.0",
    "jest": "^23.5.0",
    "jest-mock-axios": "^2.1.11",
    "json-server": "^0.14.0",
    "lodash-webpack-plugin": "^0.11.5",
    "lodash.merge": "^4.6.1",
    "mockdate": "^2.0.2",
    "next": "canary",
    "next-redux-saga": "^3.0.0-beta.1",
    "next-redux-wrapper": "^2.0.0",
    "next-routes": "^1.4.2",
    "node-sass": "^4.9.3",
    "normalize-scss": "^7.0.1",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pm2": "^3.0.4",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "prettier-package-json": "^2.0.0",
    "prop-types": "^15.6.2",
    "raw-loader": "^0.5.1",
    "react": "^16.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.2",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.1",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-saga": "^0.16.0",
    "redux-saga-tester": "^1.0.379",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.0",
    "svg-loader": "^0.0.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-reducers": "^0.4.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,jsx}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  }
}
