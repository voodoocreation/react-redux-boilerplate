{
  "name": "react-redux-boilerplate",
  "author": "Raice Hannay <voodoocreation@gmail.com>",
  "description": "A React Redux boilerplate with server rendering.",
  "license": "ISC",
  "version": "1.0.0",
  "keywords": [
    "react",
    "redux",
    "redux-saga",
    "next",
    "server rendering",
    "SSR",
    "boilerplate"
  ],
  "scripts": {
    "dev": "cross-env PORT=5000 node --icu-data-dir=node_modules/full-icu next.server.js",
    "build": "npm-run-all clean build:prod",
    "build:prod": "next build",
    "build:export": "npm-run-all build export",
    "export": "next export",
    "clean": "rimraf dist",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
    "start": "cross-env pm2 start start.config.json --env=production",
    "stop": "cross-env pm2 kill",
    "lint": "npm-run-all --parallel lint:*",
    "lint:scss": "stylelint --config .stylelintrc \"src/**/*.+(css|scss)\"",
    "lint:ts": "tslint \"./{types,src,pages}/**/*.ts?(x)\"",
    "test": "cross-env NODE_ENV=test TZ=Pacific/Auckland NODE_ICU_DATA=node_modules/full-icu jest --no-cache --config ./jest.config.js",
    "test:all": "npm-run-all format typecheck lint test:coverage",
    "test:coverage": "cross-env NODE_ENV=test TZ=Pacific/Auckland NODE_ICU_DATA=node_modules/full-icu jest --no-cache --coverage --config ./jest.config.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@formatjs/intl-pluralrules": "^1.3.4",
    "@formatjs/intl-relativetimeformat": "^4.4.3",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/humps": "^1.1.2",
    "@types/jest": "^24.0.17",
    "@types/next-redux-saga": "^3.0.1",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-intl-redux": "^0.1.12",
    "@types/react-redux": "^7.1.1",
    "@types/redux-saga-tester": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "accepts": "^1.3.7",
    "autoprefixer": "^9.6.1",
    "axios": "^0.19.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "classnames": "^2.2.6",
    "cross-env": "^6.0.3",
    "css-mqpacker": "^7.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "full-icu": "^1.3.0",
    "glob": "^7.1.4",
    "humps": "^2.0.1",
    "jest": "^24.8.0",
    "json-server": "^0.15.0",
    "mockdate": "^2.0.4",
    "next": "^9.0.3",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^4.0.1",
    "next-routes": "^1.4.2",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pm2": "^4.1.2",
    "postcss-easy-import": "^3.0.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-intl": "^3.5.0",
    "react-intl-redux": "^2.2.0",
    "react-redux": "^7.1.3",
    "react-test-wrapper": "^2.0.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "redux-saga-tester": "^1.0.631",
    "reselect": "^4.0.0",
    "rimraf": "^3.0.0",
    "scrollbar-size": "^2.1.0",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.13.0",
    "svg-loader": "^0.0.2",
    "ts-deepmerge": "^1.0.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.3",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.1",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
