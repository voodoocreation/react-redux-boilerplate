{
  "name": "react-redux-boilerplate",
  "author": "Raice Hannay <voodoocreation@gmail.com>",
  "description": "A React Redux boilerplate with server rendering.",
  "license": "ISC",
  "version": "1.0.0",
  "keywords": [
    "react",
    "redux",
    "redux-saga",
    "next",
    "server rendering",
    "SSR",
    "boilerplate"
  ],
  "scripts": {
    "dev": "cross-env PORT=5000 node --icu-data-dir=node_modules/full-icu next.server.js",
    "build": "npm-run-all clean build:prod",
    "build:prod": "next build",
    "build:export": "npm-run-all build export",
    "export": "next export",
    "clean": "rimraf dist",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
    "start": "cross-env pm2 start start.config.json --env=production",
    "stop": "cross-env pm2 kill",
    "lint": "npm-run-all --parallel typecheck lint:ts lint:scss",
    "lint:fix": "npm-run-all --parallel format typecheck lint:ts:fix lint:scss",
    "lint:scss": "stylelint --config .stylelintrc \"src/**/*.+(css|scss)\"",
    "lint:ts": "eslint \"./{types,src,pages}/**/*.ts?(x)\"",
    "lint:ts:fix": "eslint \"./{types,src,pages}/**/*.ts?(x)\" --fix",
    "test": "cross-env NODE_ENV=test TZ=Pacific/Auckland NODE_ICU_DATA=node_modules/full-icu jest --no-cache --config ./jest.config.js",
    "test:all": "npm-run-all lint:fix test:coverage",
    "test:coverage": "cross-env NODE_ENV=test TZ=Pacific/Auckland NODE_ICU_DATA=node_modules/full-icu jest --no-cache --coverage --config ./jest.config.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@formatjs/intl-pluralrules": "^1.5.2",
    "@formatjs/intl-relativetimeformat": "^4.5.9",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/humps": "^1.1.3",
    "@types/jest": "^25.1.3",
    "@types/next-redux-saga": "^3.0.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-intl-redux": "^0.1.12",
    "@types/react-redux": "^7.1.7",
    "@types/redux-saga-tester": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@zeit/next-sass": "^1.0.1",
    "accepts": "^1.3.7",
    "autoprefixer": "^9.7.4",
    "axios": "^0.19.2",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.1",
    "css-mqpacker": "^7.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-voodoocreation": "^1.0.9",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "full-icu": "^1.3.1",
    "glob": "^7.1.4",
    "humps": "^2.0.1",
    "jest": "^25.1.0",
    "jest-mocks": "^1.0.3",
    "json-server": "^0.16.1",
    "mockdate": "^2.0.4",
    "next": "^9.3.2",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^5.0.0",
    "next-routes": "^1.4.2",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pm2": "^4.2.3",
    "postcss-easy-import": "^3.0.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-intl": "^3.12.1",
    "react-intl-redux": "^2.2.0",
    "react-redux": "^7.2.0",
    "react-test-wrapper": "^2.0.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "redux-saga-tester": "^1.0.705",
    "reselect": "^4.0.0",
    "rimraf": "^3.0.2",
    "scrollbar-size": "^2.1.0",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.14.2",
    "svg-loader": "^0.0.2",
    "ts-deepmerge": "^1.0.4",
    "typescript": "3.7.5",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.1",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
