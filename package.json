{
  "name": "react-redux-boilerplate",
  "author": "Raice Hannay <voodoocreation@gmail.com>",
  "description": "A React Redux boilerplate with server rendering.",
  "license": "ISC",
  "version": "1.0.0",
  "keywords": [
    "react",
    "redux",
    "redux-saga",
    "next",
    "server rendering",
    "SSR",
    "boilerplate"
  ],
  "scripts": {
    "dev": "cross-env PORT=5000 node --icu-data-dir=node_modules/full-icu next.server.js",
    "build": "npm-run-all clean build:prod",
    "build:prod": "next build",
    "clean": "rimraf dist",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
    "start": "cross-env pm2 start start.config.json --env=production",
    "stop": "cross-env pm2 kill",
    "lint": "npm-run-all --parallel lint:*",
    "lint:scss": "stylelint --config .stylelintrc \"src/**/*.+(css|scss)\"",
    "lint:ts": "tslint \"./{types,src,pages}/**/*.ts?(x)\"",
    "precommit": "lint-staged",
    "test": "cross-env NODE_ENV=test jest --no-cache --config ./jest.config.js",
    "test:all": "npm-run-all format typecheck lint test:coverage",
    "test:coverage": "cross-env NODE_ENV=test jest --no-cache --env=jsdom --coverage --config ./jest.config.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@types/classnames": "^2.2.8",
    "@types/enzyme": "^3.9.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/humps": "^1.1.2",
    "@types/jest": "^24.0.15",
    "@types/lodash.merge": "^4.6.6",
    "@types/next": "^8.0.5",
    "@types/next-redux-saga": "^3.0.1",
    "@types/next-redux-wrapper": "^2.0.2",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-intl": "^2.3.18",
    "@types/react-intl-redux": "^0.1.11",
    "@types/react-redux": "^7.1.1",
    "@types/redux-saga-tester": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "accepts": "^1.3.7",
    "autoprefixer": "^9.6.0",
    "axios": "^0.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "css-mqpacker": "^7.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "full-icu": "^1.3.0",
    "glob": "^7.1.4",
    "humps": "^2.0.1",
    "jest": "^24.8.0",
    "json-server": "^0.15.0",
    "lodash-webpack-plugin": "^0.11.5",
    "lodash.merge": "^4.6.1",
    "mockdate": "^2.0.3",
    "next": "^8.1.0",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^3.0.0-alpha.3",
    "next-routes": "^1.4.2",
    "next-server": "^8.1.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pm2": "^3.5.1",
    "postcss-easy-import": "^3.0.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^2.9.0",
    "react-intl-redux": "^2.1.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.0",
    "redux-saga": "^1.0.3",
    "redux-saga-tester": "^1.0.486",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.3",
    "scrollbar-size": "^2.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0",
    "stylelint-scss": "^3.8.0",
    "svg-loader": "^0.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-reducers": "^1.2.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,jsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
